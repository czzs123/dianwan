/*轮播图banner:儿子们*/
.smallbanner{
 width:352px;
 height:414px;
 float:left;
 margin:0px 4px;
 position:relative;
}
.smallbanner img{
  transition:all 1s;
}
.smallbanner img:hover{
   transform:scale(1.1,1.1);
}
.bigbanner{
 width:720px;
 height:414px;
 background:green;
 float:left;
 margin:0px 4px;
 position:relative;
 overflow:hidden;
}
.bigbanner img{
  transition:all 1s;
}
.bigbanner img:hover{
   transform:scale(1.1,1.1);
}

