/*HOMEGAME:游戏内容*/
.home-game__content{
  width:1200px;
  height:204px;
  margin:auto;
  margin-top:50px;
}
.home-game__content:after{
  content:"";
  display:block;
}
/*HOMEGAME:游戏内容:游戏卡片*/
.home-game__content>.home-list-card{
  margin:0px;padding:0px;
  width:800px;
  height:204px;
